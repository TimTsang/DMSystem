a {
	color : #15428B;
}
a:link,
a:visited {
	text-decoration : none;
}

a:hover {
	text-decoration : underline;
}

#header {
	background : #7F99BE url(../images/layout-browser-hd-bg.gif) repeat-x center;
	visibility : hidden;
}

#header h1 {
	font-size   : 16px;
	font-weight : bold;
	color       : #fff;
	padding     : 5px 10px;
}

#footer {
	position   : absolute;
	visibility : hidden;
	bottom     : 0;
	width      : 100%;
	height     : 60px;/*脚部的高度*/ 
	background : #7F99BE url(../images/layout-browser-hd-bg.gif) repeat-x center;
	clear      :both;
	text-align :center;
}
 #footer h1{    
  	margin  : 0;  
  	padding : 010px;  
